p5.key) {\n      case \"ArrowUp\":\n      case \"ArrowDown\":\n        paddleRight.move(0);\n        break;\n      case \"w\":\n      case \"s\":\n        paddleLeft.move(0);\n        break;\n    }\n  };\n  p5.keyPressed = () => {\n    if (!pong) {\n      return;\n    }\n    const paddleLeft = pong.player(\"1\");\n    const paddleRight = pong.player(\"2\");\n\n    switch (p5.key) {\n      case \"ArrowUp\":\n        paddleRight.move(-10);\n        break;\n      case \"ArrowDown\":\n        paddleRight.move(10);\n        break;\n      case \"w\":\n        paddleLeft.move(-10);\n        break;\n      case \"s\":\n        paddleLeft.move(10);\n        break;\n    }\n  };\n};\n\nnew P5(sketch);\n"},"lineCount":null}},"error":null,"hash":"d05af05f5bac57548661855ebe0ccd43","cacheData":{"env":{}}}